<% if object && object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      <%= t ".error-exaplanation", number_errors: pluralize(object.errors.count, t(".error")) %>.
      <ul>
      <% object.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  </div>
<% end %>
